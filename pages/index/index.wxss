.title {
  display: flex;
  flex-direction: row;
  justify-content: center;
  font-size: 30px;
  color: #FFF5AB;
  text-shadow: 1px 3px 0 #969696, 1px 13px 5px #aba8a8;
  margin-top: 30px;
}

.myimg {
  margin: 80px;
  background-color: #FFCEAD;
  border-radius: 20px;
}

.mybtn {
  padding: 10px 20px;
  width: 50%;
  background-color: #C449C2;
  color: #fff;
  border-width: 0px;
  box-shadow: 0 19px 38px rgba(0,0,0,0.30), 0 15px 12px rgba(0,0,0,0.22); 
}